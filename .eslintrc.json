{
  "env": {
    "commonjs": true,
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": 2,
    "no-var": 2,
    "no-unused-vars": 1,
    "no-multiple-empty-lines": 1,
    "no-console": 0,
    "object-shorthand": 0,
    "class-methods-use-this": 0,
    "linebreak-style": 0,
    "func-names": 0,
    "global-require": 0,
    "camelcase": 0,
    "import/no-dynamic-require": 0,
    "import/no-extraneous-dependencies": 0
  }
}
